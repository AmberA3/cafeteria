<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table>
        <thead>
          <th>ID</th>
          <th>Cohort</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Date</th>
        </thead>
        <tbody>
        <% 
          if(trainees.length){
          
          for(var i = 0; i< trainees.length; i++) {
        %> 
          <tr>
            <td><%= trainees[i].id %></td>
            <td><%= trainees[i].cohort %></td>
            <td><%= trainees[i].firstNm %></td>
            <td><%= trainees[i].lastNm %></td>
            <td><%= trainees[i].date_joined.toLocaleDateString() %></td>
          </tr>
        <% } 
        }else { %>
            <tr>
              <td colspan="3">No Records Found</td>
            </tr>
        <% } %>
        </tbody>
      </table>
</body>
</html>